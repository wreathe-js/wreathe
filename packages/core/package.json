{
  "name": "@wreathe-js/core",
  "version": "0.1.0-alpha.1",
  "description": "Connects the engine to the transmission.",
  "keywords": [
    "laravel",
    "preact",
    "react",
    "spa",
    "ssr",
    "typescript",
    "vite",
    "vue",
    "wreathe",
    "wreathe-js"
  ],
  "homepage": "https://wreathe.dev",
  "bugs": {
    "url": "https://github.com/wreathe-js/wreathe/issues",
    "email": "contact@wreathe.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wreathe-js/wreathe.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "require": "./dist/server.cjs",
      "import": "./dist/server.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "server": [
        "dist/server.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "deepmerge": "^4.3.0",
    "nprogress": "^0.2.0",
    "qs": "^6.11.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
