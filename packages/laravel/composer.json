{
    "name": "wreathe-js/laravel",
    "type": "library",
    "description": "The Laravel adapter for wreathe.",
    "keywords": ["laravel", "wreathe-js", "wreathe"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wreathe\\": "src"
        },
        "files": [
            "./helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Wreathe\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "ext-json": "*",
        "laravel/framework": "^9.0|^10.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "orchestra/testbench": "^7.0|^8.0",
        "mockery/mockery": "^1.3.3",
        "phpunit/phpunit": "^8.0|^9.5.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wreathe\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
