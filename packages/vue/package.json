{
  "name": "@wreathe-js/vue",
  "version": "0.1.0-alpha.1",
  "description": "Connects the engine to the transmission.",
  "keywords": [
    "laravel", "preact", "react", "spa", "ssr", "typescript", "vite", "vue",
    "wreathe", "wreathe-js"
  ],
  "homepage": "https://wreathe.dev",
  "bugs": {
    "url": "https://github.com/wreathe-js/wreathe/issues",
    "email": "contact@wreathe.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wreathe-js/wreathe.git",
    "directory": "packages/vue"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "require": "./dist/server.cjs",
      "import": "./dist/server.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": ["dist/index.d.ts"],
      "server": ["dist/server.d.ts"]
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@wreathe-js/core": "workspace:*",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0"
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^18.15.11",
    "axios": "^1.3.4",
    "vue": "^3.2.47"
  },
  "peerDependencies": {
    "vue": "^3.2.47"
  },
  "publishConfig": {
    "access": "public"
  }
}
